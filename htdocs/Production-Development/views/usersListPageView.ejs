<div class="">
    <div class="row">

        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>Users<small>subtitle</small></h2>
                    <div class="text-right admin-profile">
                        <a href="/users/addNewUsersView" class="btn btn-success">
                            <i class="fa fa-plus m-right-xs"></i> Add User
                        </a>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content overflow-x">

                    <table class="table table-striped table-bordered">
                        <thead>
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Edit</th>
                            <th>User Name</th>
                            <th>Address</th>
                            <th>Email</th>
                            <th>Contact</th>
                            <th>Gender</th>
                            <th>Designation</th>
                            <th>Joining Date</th>
                            <!--<th>Termination Date</th>-->
                            <th>Status</th>
                            <th>Company</th>
                            <th>Role</th>


                        </tr>
                        </thead>
                        <tbody>

                        <% for(var i=0; i < userList.length; i++) { %>
                        <tr>
                            <th scope="row"><%= i+1 %></th>
                            <td><%= userList[i].first_name %>  <%= userList[i].last_name %></td>
                            <td><a href="/users/viewUserProfile/<%=userList[i].login_id%>">View</a> / <a href="/users/userProfile/<%=userList[i].login_id%>">Edit</a></td>
                            <td><%= userList[i].login_id %></td>
                            <td><%= userList[i].address %></td>
                            <td><%= userList[i].email %></td>
                            <td><%= userList[i].contact_no %></td>
                            <td><%= userList[i].gender %></td>
                            <td><%= userList[i].designation %></td>
                            <%  var jdate = userList[i].joining_date+"";
                                jdate = jdate.substring(4,15);
                            %>
                            <td><%= jdate %></td>
                            <!-- <td><%= userList[i].termination_date %></td> -->

                            <% if (userList[i].is_active == 1) { %>
                            <td><%= 'Active' %></td>
                            <% } else if (userList[i].is_active == 0) { %>
                            <td><%= 'Disabled' %></td>
                            <% } %>

                            <td><%= userList[i].comp_id %></td>

                            <% if (userList[i].role_id == 1) { %>
                                <td><%= 'Admin' %></td>
                            <% } else if (userList[i].role_id == 2) { %>
                                <td><%= 'User' %></td>
                            <% } else if (userList[i].role_id == 3){ %>
                                <td><%= 'Super Admin' %></td>
                            <% } %>

                        </tr>
                        <% } %>

                        </tbody>
                    </table>

                </div>



            </div>
        </div>

    </div>
</div>